<%= form_with(model: relay_result) do |form| %>
  <% if relay_result.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(relay_result.errors.count, "error") %> prohibited this relay_result from being saved:</h2>

      <ul>
        <% relay_result.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :place %>
    <%= form.number_field :place %>
  </div>

  <div class="field">
    <%= form.label :team %>
    <%= form.text_field :team %>
  </div>

  <div class="field">
    <%= form.label :time %>
    <%= form.number_field :time %>
  </div>

  <div class="field">
    <%= form.label :category_id %>
    <%= form.text_field :category_id %>
  </div>

  <div class="field">
    <%= form.label :group_id %>
    <%= form.text_field :group_id %>
  </div>

  <div class="field">
    <%= form.label :date %>
    <%= form.date_select :date %>
  </div>

  <div class="field">
    <%= form.label :results %>
    <%= form.text_field :results %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
