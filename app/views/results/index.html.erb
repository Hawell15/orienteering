<p id="notice"><%= notice %></p>

<h1>Results</h1>

<table>
  <thead>
    <tr>
      <th>Place</th>
      <th>Runner</th>
      <th>Time</th>
      <th>Category</th>
      <th>Group</th>
      <th>Wre points</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result.place %></td>
        <td><%= result.runner_id %></td>
        <td><%= result.time %></td>
        <td><%= result.category_id %></td>
        <td><%= result.group_id %></td>
        <td><%= result.wre_points %></td>
        <td><%= result.date %></td>
        <td><%= link_to 'Show', result %></td>
        <td><%= link_to 'Edit', edit_result_path(result) %></td>
        <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result', new_result_path %>
